<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY stix "http://docs.oasis-open.org/ns/cti/stix#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="http://docs.oasis-open.org/ns/cti/stix"
	xmlns="http://docs.oasis-open.org/ns/cti/stix#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:stix="http://docs.oasis-open.org/ns/cti/stix#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="http://docs.oasis-open.org/ns/cti/stix">
		<rdfs:comment xml:lang="en-us">This ontology is the master ontology  for the STIX 2.1.0.  It imports all the various STIX ontologies files to create an unified ontology based on the various component ontologies that make up STIX.</rdfs:comment>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/data-marking"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/adversary"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/artifact"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/attack-pattern"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/autonomous-system"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/campaign"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/core"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/course-of-action"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/directory"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/domain-name"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/email-address"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/file"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/grouping"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/identity"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/incident"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/indicator"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/infrastructure"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/ip-address"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/language-content"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/location"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/mac-address"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/malware"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/message"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/mutex"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/network-traffic"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/note"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/observed-data"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/opinion"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/process"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/relationship"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/report"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/sighting"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/software"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/tool"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/uri"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/user-account"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/vulnerability"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/windows-registry"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/x509-certificate"/>
		<owl:versionInfo>2.1.0</owl:versionInfo>
	</owl:Ontology>
	
	<rdf:Description rdf:about="&stix;Incident">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stix;criticality"/>
				<owl:someValuesFrom rdf:resource="&xsd;string"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stix;detection_methods"/>
				<owl:someValuesFrom rdf:resource="&xsd;string"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stix;determination"/>
				<owl:someValuesFrom rdf:resource="&xsd;string"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stix;extension_type"/>
				<owl:someValuesFrom rdf:resource="&xsd;string"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stix;external_impacts"/>
				<owl:someValuesFrom rdf:resource="&xsd;string"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stix;functional_impact"/>
				<owl:someValuesFrom rdf:resource="&xsd;string"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stix;incident_types"/>
				<owl:someValuesFrom rdf:resource="&xsd;string"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stix;investigation_status"/>
				<owl:someValuesFrom rdf:resource="&xsd;string"/>
			</owl:Restriction>
		</rdfs:subClassOf>
	</rdf:Description>
	
	<owl:DatatypeProperty rdf:about="&stix;attcker_activities">
		<rdfs:range rdf:resource="&xsd;string"/>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&stix;criticality">
		<rdfs:range rdf:resource="&xsd;string"/>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&stix;defender_activities">
		<rdfs:range>
			<rdfs:Datatype>
				<owl:oneOf>
					<rdf:List>
						<rdf:first>containment-completed</rdf:first>
						<rdf:rest>
							<rdf:List>
								<rdf:first>containment-started</rdf:first>
								<rdf:rest>
									<rdf:List>
										<rdf:first>declared</rdf:first>
										<rdf:rest>
											<rdf:List>
												<rdf:first>detected</rdf:first>
												<rdf:rest>
													<rdf:List>
														<rdf:first>eradication-completed</rdf:first>
														<rdf:rest>
															<rdf:List>
																<rdf:first>eradication-started</rdf:first>
																<rdf:rest>
																	<rdf:List>
																		<rdf:first>escalated</rdf:first>
																		<rdf:rest>
																			<rdf:List>
																				<rdf:first>recovery-completed</rdf:first>
																				<rdf:rest>
																					<rdf:List>
																						<rdf:first>recovery-started</rdf:first>
																						<rdf:rest>
																							<rdf:List>
																								<rdf:first>reported</rdf:first>
																								<rdf:rest rdf:resource="&rdf;nil"/>
																							</rdf:List>
																						</rdf:rest>
																					</rdf:List>
																				</rdf:rest>
																			</rdf:List>
																		</rdf:rest>
																	</rdf:List>
																</rdf:rest>
															</rdf:List>
														</rdf:rest>
													</rdf:List>
												</rdf:rest>
											</rdf:List>
										</rdf:rest>
									</rdf:List>
								</rdf:rest>
							</rdf:List>
						</rdf:rest>
					</rdf:List>
				</owl:oneOf>
			</rdfs:Datatype>
		</rdfs:range>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&stix;detection_methods">
		<rdfs:range>
			<rdfs:Datatype>
				<owl:oneOf>
					<rdf:List>
						<rdf:first>automated-tool</rdf:first>
						<rdf:rest>
							<rdf:List>
								<rdf:first>human-review</rdf:first>
								<rdf:rest>
									<rdf:List>
										<rdf:first>message-from-attacker</rdf:first>
										<rdf:rest>
											<rdf:List>
												<rdf:first>system-outage</rdf:first>
												<rdf:rest>
													<rdf:List>
														<rdf:first>user-reporting</rdf:first>
														<rdf:rest rdf:resource="&rdf;nil"/>
													</rdf:List>
												</rdf:rest>
											</rdf:List>
										</rdf:rest>
									</rdf:List>
								</rdf:rest>
							</rdf:List>
						</rdf:rest>
					</rdf:List>
				</owl:oneOf>
			</rdfs:Datatype>
		</rdfs:range>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&stix;determination">
		<rdfs:range>
			<rdfs:Datatype>
				<owl:oneOf>
					<rdf:List>
						<rdf:first>confirmed</rdf:first>
						<rdf:rest>
							<rdf:List>
								<rdf:first>false-positive</rdf:first>
								<rdf:rest>
									<rdf:List>
										<rdf:first>suspected</rdf:first>
										<rdf:rest rdf:resource="&rdf;nil"/>
									</rdf:List>
								</rdf:rest>
							</rdf:List>
						</rdf:rest>
					</rdf:List>
				</owl:oneOf>
			</rdfs:Datatype>
		</rdfs:range>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&stix;extension_type">
		<rdfs:range>
			<rdfs:Datatype>
				<owl:oneOf>
					<rdf:List>
						<rdf:first>legacy</rdf:first>
						<rdf:rest>
							<rdf:List>
								<rdf:first>new-sco</rdf:first>
								<rdf:rest>
									<rdf:List>
										<rdf:first>new-sdo</rdf:first>
										<rdf:rest>
											<rdf:List>
												<rdf:first>new-sro</rdf:first>
												<rdf:rest>
													<rdf:List>
														<rdf:first>property-extension</rdf:first>
														<rdf:rest>
															<rdf:List>
																<rdf:first>toplevel-property-extension</rdf:first>
																<rdf:rest rdf:resource="&rdf;nil"/>
															</rdf:List>
														</rdf:rest>
													</rdf:List>
												</rdf:rest>
											</rdf:List>
										</rdf:rest>
									</rdf:List>
								</rdf:rest>
							</rdf:List>
						</rdf:rest>
					</rdf:List>
				</owl:oneOf>
			</rdfs:Datatype>
		</rdfs:range>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&stix;external_impacts">
		<rdfs:range>
			<rdfs:Datatype>
				<owl:oneOf>
					<rdf:List>
						<rdf:first>civil-liberties</rdf:first>
						<rdf:rest>
							<rdf:List>
								<rdf:first>economic</rdf:first>
								<rdf:rest>
									<rdf:List>
										<rdf:first>foreign-relations</rdf:first>
										<rdf:rest>
											<rdf:List>
												<rdf:first>national-security</rdf:first>
												<rdf:rest>
													<rdf:List>
														<rdf:first>public-confidence</rdf:first>
														<rdf:rest>
															<rdf:List>
																<rdf:first>public-health</rdf:first>
																<rdf:rest rdf:resource="&rdf;nil"/>
															</rdf:List>
														</rdf:rest>
													</rdf:List>
												</rdf:rest>
											</rdf:List>
										</rdf:rest>
									</rdf:List>
								</rdf:rest>
							</rdf:List>
						</rdf:rest>
					</rdf:List>
				</owl:oneOf>
			</rdfs:Datatype>
		</rdfs:range>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&stix;functional_impact">
		<rdfs:range>
			<rdfs:Datatype>
				<owl:oneOf>
					<rdf:List>
						<rdf:first>denial</rdf:first>
						<rdf:rest>
							<rdf:List>
								<rdf:first>loss-of-control</rdf:first>
								<rdf:rest>
									<rdf:List>
										<rdf:first>minimal</rdf:first>
										<rdf:rest>
											<rdf:List>
												<rdf:first>none</rdf:first>
												<rdf:rest>
													<rdf:List>
														<rdf:first>significant</rdf:first>
														<rdf:rest rdf:resource="&rdf;nil"/>
													</rdf:List>
												</rdf:rest>
											</rdf:List>
										</rdf:rest>
									</rdf:List>
								</rdf:rest>
							</rdf:List>
						</rdf:rest>
					</rdf:List>
				</owl:oneOf>
			</rdfs:Datatype>
		</rdfs:range>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&stix;incident_types">
		<rdfs:range>
			<rdfs:Datatype>
				<owl:oneOf>
					<rdf:List>
						<rdf:first>blocked</rdf:first>
						<rdf:rest>
							<rdf:List>
								<rdf:first>compromised-system</rdf:first>
								<rdf:rest>
									<rdf:List>
										<rdf:first>destruction</rdf:first>
										<rdf:rest>
											<rdf:List>
												<rdf:first>equipment-loss</rdf:first>
												<rdf:rest>
													<rdf:List>
														<rdf:first>failed-attempt</rdf:first>
														<rdf:rest>
															<rdf:List>
																<rdf:first>major</rdf:first>
																<rdf:rest>
																	<rdf:List>
																		<rdf:first>supply-chain-customer</rdf:first>
																		<rdf:rest>
																			<rdf:List>
																				<rdf:first>supply-chain-vendor</rdf:first>
																				<rdf:rest>
																					<rdf:List>
																						<rdf:first>unauthorized-access</rdf:first>
																						<rdf:rest>
																							<rdf:List>
																								<rdf:first>unauthorized-release</rdf:first>
																								<rdf:rest>
																									<rdf:List>
																										<rdf:first>under-investigation</rdf:first>
																										<rdf:rest rdf:resource="&rdf;nil"/>
																									</rdf:List>
																								</rdf:rest>
																							</rdf:List>
																						</rdf:rest>
																					</rdf:List>
																				</rdf:rest>
																			</rdf:List>
																		</rdf:rest>
																	</rdf:List>
																</rdf:rest>
															</rdf:List>
														</rdf:rest>
													</rdf:List>
												</rdf:rest>
											</rdf:List>
										</rdf:rest>
									</rdf:List>
								</rdf:rest>
							</rdf:List>
						</rdf:rest>
					</rdf:List>
				</owl:oneOf>
			</rdfs:Datatype>
		</rdfs:range>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&stix;investigation_status">
		<rdfs:range>
			<rdfs:Datatype>
				<owl:oneOf>
					<rdf:List>
						<rdf:first>closed</rdf:first>
						<rdf:rest>
							<rdf:List>
								<rdf:first>new</rdf:first>
								<rdf:rest>
									<rdf:List>
										<rdf:first>open</rdf:first>
										<rdf:rest rdf:resource="&rdf;nil"/>
									</rdf:List>
								</rdf:rest>
							</rdf:List>
						</rdf:rest>
					</rdf:List>
				</owl:oneOf>
			</rdfs:Datatype>
		</rdfs:range>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&stix;mitigation">
		<rdfs:comment>A description of the steps taken to mitigate this incident.

If available this can be further detailed within defender activity, but this captures a high-level narrative of what has been done instead of a more step by step chain of action.</rdfs:comment>
		<rdfs:range rdf:resource="&xsd;string"/>
	</owl:DatatypeProperty>
	
	<rdf:Description rdf:about="http://docs.oasis-open.org/ns/cti/stix/core#Object">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stix;defanged"/>
				<owl:someValuesFrom rdf:resource="&xsd;boolean"/>
			</owl:Restriction>
		</rdfs:subClassOf>
	</rdf:Description>

</rdf:RDF>